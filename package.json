{
  "name": "labcodes-cli",
  "version": "1.1.0",
  "description": "Labcodes CLI",
  "main": "lib/index.js",
  "scripts": {
    "start:dev": "node ./index.js labcodes start-project",
    "build": "npm run clear && npm run lint && npm run babel && npm run webpack:umd",
    "clear": "rimraf dist && rimraf dale",
    "lint": "./node_modules/.bin/eslint lib/*.js",
    "babel": "./node_modules/.bin/babel lib --out-dir dist",
    "webpack:umd": "./node_modules/.bin/webpack -p"
  },
  "author": "Thulio Philipe <thulio@labcodes.com.br>",
  "license": "MIT",
  "bin": {
    "labcodes-cli": "./index.js"
  },
  "files": [
    "dist",
    "lib"
  ],
  "preferGlobal": true,
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.10.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  },
  "dependencies": {
    "@octokit/rest": "^15.2.6",
    "chalk": "^2.3.2",
    "clear": "^0.1.0",
    "figlet": "^1.2.0",
    "inquirer": "^5.2.0",
    "minimist": "^1.2.0",
    "simple-git": "^1.92.0"
  },
  "engines": {
    "node": ">= 8.10.0",
    "npm": ">= 5.6.0"
  }
}
